{
  "name": "diazdavid-info/slim-php-boilerplate",
  "description": "Project boilerplate with slim, dbal and symfony console",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "David Diaz",
      "email": "diazdavid.info@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "slim/slim": "^4.7",
    "doctrine/dbal": "^2.12",
    "symfony/console": "^5.2",
    "doctrine/migrations": "^3.1",
    "vlucas/phpdotenv": "^5.3",
    "slim/psr7": "^1.3",
    "php-di/slim-bridge": "^3.1",
    "ramsey/uuid": "^4.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Api\\": "app/api"
    },
    "files": [
      "src/Shared/utils.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
